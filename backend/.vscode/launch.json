{
  "version": "0.2.0",
  "configurations": [{
      "type": "node",
      "request": "launch",
      "name": "Jest Tests",
      "program": "${workspaceRoot}\\node_modules\\jest\\bin\\jest.js",
      "args": [
        "-i"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "outFiles": [
        "${workspaceRoot}/src/**/*"
      ],
      "envFile": "${workspaceRoot}/.env"
    },
    {
      "type": "pwa-node",
      "request": "attach",
      "restart": true,
      "name": "Debug",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.ts"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Tests",
      "program": "${workspaceRoot}\\node_modules\\jest\\bin\\jest.js",
      "args": [
        "-i"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "outFiles": [
        "${workspaceRoot}/src/**/*"
      ],
      "envFile": "${workspaceRoot}/.env"
    },
  ]
}
